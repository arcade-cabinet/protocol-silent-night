{
  "expo": {
    "name": "Protocol: Silent Night",
    "slug": "protocol-silent-night",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "scheme": "protocol-silent-night",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "owner": "jbcom",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/protocol-silent-night",
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jbcom.protocolsilentnight",
      "buildNumber": "1.0.0",
      "requireFullScreen": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a1a"
      },
      "package": "com.jbcom.protocolsilentnight",
      "versionCode": 1,
      "resizeableActivity": true,
      "allowBackup": false
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-haptics"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "protocol-silent-night"
      },
      "targetDevices": {
        "pixel8a": {
          "name": "Google Pixel 8a",
          "width": 1080,
          "height": 2400,
          "density": 2.625
        },
        "oneplusOpen": {
          "name": "OnePlus Open",
          "folded": {
            "width": 1116,
            "height": 2484
          },
          "unfolded": {
            "width": 2268,
            "height": 2076
          },
          "density": 2.75,
          "foldable": true
        },
        "iphone16ProMax": {
          "name": "iPhone 16 Pro Max",
          "width": 1320,
          "height": 2868,
          "density": 3.0
        },
        "ipadPro13": {
          "name": "iPad Pro 13-inch",
          "width": 2064,
          "height": 2752,
          "density": 2.0
        }
      }
    }
  }
}
