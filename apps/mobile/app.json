{
  "expo": {
    "name": "Protocol: Silent Night",
    "slug": "protocol-silent-night",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "scheme": "protocol-silent-night",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a0a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jbcom.protocolsilentnight",
      "buildNumber": "1.0.0",
      "requireFullScreen": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a0a1a"
      },
      "package": "com.jbcom.protocolsilentnight",
      "versionCode": 1
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-haptics"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "protocol-silent-night"
      }
    }
  }
}
